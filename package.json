{
  "name": "speed-dial",
  "description": "Speed dial page",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "build": "ng build --prod --progress",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean:dist": "rimraf dist",
    "clean:node": "rimraf node_modules",
    "clean:all": "concurrently \"yarn clean:node\" \"yarn clean:dist\"",
    "service:create": "node util/createService.js",
    "service:remove": "node util/removeService.js",
    "start": "serve -n -l 8080 dist/",
    "postinstall": "yarn build && yarn service:create",
    "preuninstall": "yarn clean:all && yarn service:remove"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.1",
    "@angular/common": "^7.1.1",
    "@angular/compiler": "^7.1.1",
    "@angular/core": "^7.1.1",
    "@angular/forms": "^7.1.1",
    "@angular/http": "^7.1.1",
    "@angular/platform-browser": "^7.1.1",
    "@angular/platform-browser-dynamic": "^7.1.1",
    "@angular/router": "^7.1.1",
    "concurrently": "^4.1.0",
    "core-js": "^2.5.7",
    "node-windows": "^0.1.14",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "serve": "^10.1.1",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "^7.1.0",
    "@angular/compiler-cli": "^7.1.1",
    "@angular/language-service": "^7.1.1",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "@types/jasmine": "^3.3.0",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.10",
    "bootstrap": "^4.1.3",
    "codelyzer": "^4.5.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng2-dragula": "^2.1.1",
    "protractor": "^5.4.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": ">=3.1.1 <3.2",
    "uuid": "^3.3.2"
  }
